package com.driver;
import com.cardetails.*;
public class Output {
    public static void printCarDetails(Car car) {
        System.out.println("Car Model: " + car.getModel());
        System.out.println("Car Type: " + car.getType());
        System.out.println("Insurance Type: " + car.getInsuranceType());
        System.out.println("Total Insurance to be Paid: " + car.calculateInsurancePremium());
    }
}
