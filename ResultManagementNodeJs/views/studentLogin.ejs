<%- include('partials/_header') %>
    <%- include('partials/_navbar') %>
        <div class="container mt-5 student-login-container">
            <form action="/student-detail" method="get" class="mb-5 student-login-form">
                <h3 class="mb-4 text-center">Find Result</h3>
                <div class="form-group student-login-from-grp">
                    <label for="rollno" class="student-login-label">Roll Number:</label>
                    <input type="text" class="form-control student-login-input" id="rollno" name="rollno" required>
                </div>
                <div class="form-group student-login-from-grp">
                    <label for="dob" class="student-login-label">Date of Birth:</label>
                    <input type="date" class="form-control student-login-input" id="dob" name="dob" required
                        min="1995-01-01" max="2016-12-31">
                </div>
                <div class="student-login-btn-container">
                    <button type="submit" class="student-login-btn">Find Result</button>
                    <a href="/" class="m-3 student-login-btn student-login-back">Back</a>
                </div>
            </form>
        </div>

        <%- include('partials/_footer') %>